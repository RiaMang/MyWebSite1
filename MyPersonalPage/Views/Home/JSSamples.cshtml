			status=false;
		}
	}
}

function armNum(num) {
	var sum=0;
	var aray = [];
	var str = num.toString();
	for (i=0; i<str.length; i++) { // cube of each digit
		aray[i] = parseInt(str.charAt(i)) * parseInt(str.charAt(i)) * parseInt(str.charAt(i));
		}
	for (j=0; j<aray.length; j++)	{ //sum of cubes of all digits
		sum+=aray[j];
		aray[j]=0;
	}
	if (sum === num) {
		return true;
	} else {
		return false;
	}
	}
