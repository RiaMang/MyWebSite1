	function factorial(num) {
		var factrl = 1;
		if(num < 1) {
			$("#resultf").text("The number must be a positive integer.");
			return;
		}
		for (i=num; i>1; i--) {
			factrl*=i;
		}
		$("#resultf").text("The factorial is: "+factrl);
	}
