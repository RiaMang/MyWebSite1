function perfectNum() {
	var num = parseInt(document.getElementById('firstNump').value);
	if(num < 1) {
		$("#resultper").text("The number must be a positive integer");
		return;
	}
	var divisors = [];
	var j=0;
	for (i=1; i<num; i++) {
		if(num%i === 0) {
		divisors[j]=i;
		j++;
		}
	}
	var sum=0;
	for (i=0; i<divisors.length; i++) {
		sum+=divisors[i];
	}
	if(sum === num){
		$("#resultper").text(num+" is a perfect number");
	} else {
		$("#resultper").text(num+" is not a perfect number");
	}
}
