	<!----PALINDROME--->
	function prepPal() {
		var myStr = document.getElementById('myText').value;
		if (myStr.length === 0) {
			$("#resultp").text("Please enter a string!");
			return;
		}
		var result = palindrome(myStr);
		if (result === true) {
			$("#resultp").text(" \'"+myStr+"\' is a palindrome.");
		} else {
			$("#resultp").text(" \'"+myStr+"\' is not a palindrome.");
		}
	}

	function palindrome(str) {
	for (a=0, b=str.length-1; a<str.length; a++,b--) {
		if(str.charAt(a) !== str.charAt(b)) {
			return false;
		}
	}
	return true;
	}
