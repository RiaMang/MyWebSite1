	function happyNum(num) {
	var sum;
	var sumAray=[];
	var aray = [];

	while (true) {
	var str = num.toString();
	sum = 0;

	for (i=0; i<str.length; i++) {
		aray[i] = parseInt(str.charAt(i)) * parseInt(str.charAt(i));
	}
	for (j=0; j<aray.length; j++)	{
		sum+=aray[j];
		aray[j]=0;
	}
	if (sum === 1) {
		sumAray.length=0;
		return true;
	} else {
		for (k=0; k<sumAray.length; k++) {
			if (sum === sumAray[k]){
				sumAray.length=0;
				return false;
			}
		}
	sumAray[sumAray.length]=sum;
	num=sum;
	}
	}
	}
